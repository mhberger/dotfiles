shopt -s histverify
source ~/.bash_aliases
source ~/.bash_git

# Bash completion â€” Ignore grailsw and grailsw.bat files
# http://blog.pclewis.com/2010/03/tip-make-bash-tab-completion-ignore-svn-directories/
export FIGNORE=lsw:lsw.bat

HISTCONTROL=ignoreboth
declare -x HISTIGNORE=tit:ls:ll:gl:gs:glg:gd:gdc:f:a:s:d:v:exit:history:

export BC_ENV_ARGS=~/.bc

export LESS="-R"

export EDITOR=/usr/bin/vim

export VAGRANT_DEFAULT_PROVIDER=vmware_fusion


# Java7
# export JAVA_HOME=/Library/Java/JavaVirtualMachines/jdk1.7.0_71.jdk/Contents/Home

# Java8
# export JAVA_HOME=/Library/Java/JavaVirtualMachines/jdk1.8.0_25.jdk/Contents/Home

# Java 7
# export JAVA_OPTS="$JAVA_OPTS -Dtomcathome=/Users/mark/tomcat -XX:MaxPermSize=256M -Xmx4096m -Dgrape.config=/Users/mark/ivy-settings.xml"

# Java 8
# export JAVA_OPTS="$JAVA_OPTS -Dtomcathome=/Users/mark/tomcat -Xmx4096m -Dgrape.config=/Users/mark/ivy-settings.xml"

export PS1='\[\033[01;30m\]\h \[\033[01;35m\]\W$(__git_ps1 " (%s)") \$ \[\033[00m\]'

set -o vi
